      {
        "id": "emperor_message_6",
        "type": "message",
        "position": [0, 15],
        "params": {
          "family": "MPLUS1p-Regular",
          "text": "実際には指輪ではなくブレスレットだったのだ",
          "size": 26,
          "color": "0xffffff",
          "anchor": [0.5, 0.5],
          "padding": 16,
          "wordWrap": true,
          "wordWrapWidth": 640
